{
    "name": "gabrielrcosta1/laravel-schema",
    "description": "Laravel package to generate migrations from a schema.db definition file.",
    "keywords": [
        "laravel",
        "migrations",
        "generator",
        "schema",
        "dsl",
        "database"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel R. Costa",
            "email": "gabrielrcosta1711@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelSchema\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSchema\\SchemaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.22"
    }
}
