{
    "name": "gabrielrcosta1/laravel-schema",
    "description": "Laravel package to generate migrations from a schema.db definition file.",
    "keywords": [
        "laravel",
        "migrations",
        "generator",
        "schema",
        "dsl"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel R. Costa",
            "email": "gabrielrcosta1711@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelSchemaPy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSchemaPy\\SchemaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
